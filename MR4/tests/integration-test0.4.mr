func test-code-flow(user Array{Int} arr)->(var Int res)
  if arr[4] > 6
    res := 6
  else-if arr?
    res := 6
  else-if arr[4] != arr[4]
    res := 6
  else-if 0 = arr[0]
    res := 6
  else
    res := 0
  do
    arr[6] := 6
    var Int x(copy arr[2])
    while x > 3 and arr is _
    var Int y(copy x - 1)
    do
      arr[4] := arr[8]
      if y > arr[4]
        continue
      var Int z(copy 7)
      if z <= arr[4]
        while z > 0
  for n in arr[2]:2 - (3 * arr[2])
    arr[0] := arr[2]
    var Int w(copy arr[0])
    if w > 4 and arr is-not arr[3:2]
      arr[1] := w


